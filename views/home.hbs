<h1>Mike Murphy's Library</h1>
<a href="/add">Add a Book to the Library</a> 
<div>
  {{#each book}}
  		<strong><h3><a id="title" name="title" href="/detail/{{title}}">{{title}}</a></h3></strong>
		<h4>By: {{name}}</h4>
		<h5>{{pages}} pages</h5>
		{{#if digital}}
		<em><p>Available for Kindle</p></em>
		{{/if}}
		<hr />
		<br>
  {{/each}}
</div>

